        <input type = "submit" value = "Remove Link">
    </form>
    <a href="/urls/<%= i %>/update">edit</a>
    <% }; %>
    
    <hr/>

    <form method = "GET" action = "/urls/new">
    <input type = "submit" value = "New">
    </form>

</body>
</html>